<!--
  An XML Catalog that rewrites any reference to 'font-config.dtd' to
  'font-config.dtd' in the local directory.
-->
<catalog  xmlns="urn:oasis:names:tc:entity:xmlns:xml:catalog">
<systemSuffix
  id = "font-config-dtd"
  systemIdSuffix = "font-config.dtd"
  uri = "font-config.dtd" />
</catalog>
